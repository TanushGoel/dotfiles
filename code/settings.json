  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "emphasis",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "strong",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "comment",
        "scope": "comment",
        "settings": {
          "foreground": "#b2b3aa7c"
        }
      },
      {
        "name": "{}",
        "scope": "constant.character",
        "settings": {
          "foreground": "#e0c99b"
        }
      },
      {
        "name": "backslash",
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "#EBCB8B"
        }
      },
      {
        "name": "number",
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#B48EAD"
        }
      },
      {
        "name": "regex",
        "scope": "constant.regexp",
        "settings": {
          "foreground": "#EBCB8B"
        }
      },
      {
        "name": "class name",
        "scope": ["entity.name.class", "entity.name.type.class"],
        "settings": {
          "foreground": "#56c2a4"
        }
      },
      {
        "name": "function name",
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "inherited class",
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#b4aa6f"
        }
      },
      {
        "name": "invalid deprecated",
        "scope": "invalid.deprecated",
        "settings": {
          "foreground": "#b2b3aa"
        }
      },
      {
        "name": "invalid illegal",
        "scope": "invalid.illegal",
        "settings": {
          "foreground": "#b2b3aa"
        }
      },
      {
        "name": "markup bold",
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "markup changed",
        "scope": "markup.changed",
        "settings": {
          "foreground": "#EBCB8B"
        }
      },
      {
        "name": "markup deleted",
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#BF616A"
        }
      },
      {
        "name": "markup inserted",
        "scope": "markup.inserted",
        "settings": {
          "foreground": "#A3BE8C"
        }
      },
      {
        "name": "punctuation - quotes, commas, etc",
        "scope": "punctuation",
        "settings": {
          "foreground": "#eceff481"
        }
      },
      {
        "name": "#",
        "scope": [
          "punctuation.definition.comment",
          "punctuation.end.definition.comment",
          "punctuation.start.definition.comment"
        ],
        "settings": {
          "foreground": "#b2b3aa7c"
        }
      },
      {
        "name": ":",
        "scope": "punctuation.section",
        "settings": {
          "foreground": "#eceff481"
        }
      },
      {
        "name": "string",
        "scope": "string",
        "settings": {
          "foreground": "#A3BE8C"
        }
      },
      {
        "name": "string regex",
        "scope": "string.regexp",
        "settings": {
          "foreground": "#EBCB8B"
        }
      },
      {
        "name": "True False math.pi math.e ?", // change
        "scope": "support.constant",
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "print len type range ?", // change
        "scope": "support.function",
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "range len min max sum ?", // change
        "scope": "support.function.construct",
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "variable",
        "scope": "variable.other",
        "settings": {
          "foreground": "#b2b3aa"
        }
      },
      {
        "name": "parameter",
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#b2b3aa" // eceff481
        }
      },
      {
        "name": "[markdown] markup fenced code block",
        "scope": [
          "text.html.markdown markup.fenced_code.block",
          "text.html.markdown markup.fenced_code.block punctuation.definition"
        ],
        "settings": {
          "foreground": "#b4aa6f"
        }
      },
      {
        "name": "[markdown] markup heading",
        "scope": "markup.heading",
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "[markdown] markup inline",
        "scope": [
          "text.html.markdown markup.inline.raw",
          "text.html.markdown markup.inline.raw punctuation.definition.raw"
        ],
        "settings": {
          "foreground": "#b4aa6f"
        }
      },
      {
        "name": "[markdown] markup italic",
        "scope": "text.html.markdown markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "[markdown] markup link",
        "scope": "text.html.markdown markup.underline.link",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "name": "[markdown] markup list numbered/unnumbered",
        "scope": "text.html.markdown beginning.punctuation.definition.list",
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "[markdown] markup quote punctuation definition",
        "scope": "text.html.markdown beginning.punctuation.definition.quote",
        "settings": {
          "foreground": "#b4aa6f"
        }
      },
      {
        "name": "[markdown] markup quote punctuation definition",
        "scope": "text.html.markdown markup.quote",
        "settings": {
          "foreground": "#b2b3aa7c"
        }
      },
      {
        "name": "[markdown] punctuation definition heading",
        "scope": "text.html.markdown punctuation.definition.heading",
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "[markdown] punctuation definition constant/string",
        "scope": [
          "text.html.markdown punctuation.definition.constant",
          "text.html.markdown punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "[markdown] string other link description/title",
        "scope": [
          "text.html.markdown constant.other.reference.link",
          "text.html.markdown string.other.link.description",
          "text.html.markdown string.other.link.title"
        ],
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "decorator",
        "scope": [
          "source.python entity.name.function.decorator",
          "source.python meta.function.decorator support.type"
        ],
        "settings": {
          "foreground": "#85bde0" // a084e0
        }
      },
      {
        "name": "function call",
        "scope": "source.python meta.function-call.generic",
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "support type",
        "scope": "source.python support.type",
        "settings": {
          "foreground": "#88c0d0bd"
        }
      },
      {
        "name": "self",
        "scope": [
          "variable.parameter.function.language.special.self.python"
        ],
        "settings": {
          "foreground": "#b8f3da88"
        }
      },
      {
        "name": "packages/libraries", // not working?
        "scope": "entity.name.namespace",
        "settings": {
          "foreground": "#9dc2d6" // 43777b 458bb6
        }
      },
      {
        "name": "constants/final vars - uppercase", // not working?
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#afcfcf" // b2ffff
        }
      },
      {
        "name": "if elif else while for break continue pass raise try except return yield from with as import class del assert finally global",
        "scope": "keyword.control",
        "settings": {
          "foreground": "#2fa2a8"
        }
      }
    ]
  },
  "explorer.confirmDelete": false,
  "notebook.confirmDeleteRunningCell": false,
  "terminal.integrated.inheritEnv": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "window.zoomLevel": 1,
  "jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
  ],
  "git.autofetch": true,
  "git.confirmSync": false,
  "notebook.lineNumbers": "on",
  "github.copilot.editor.enableAutoCompletions": true,
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.pdf": "pdf.preview"
  }
}
